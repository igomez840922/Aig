@page "/inspectionsedit/{Id:long}"
@attribute [Authorize]
@layout MainLayout
@implements IDisposable

@if (model != null)
{
    switch (model.Inspeccion.TipoActa){
        case DataModel.Helper.enumAUD_TipoActa.AF:
        case DataModel.Helper.enumAUD_TipoActa.CUF:{
                <AuditoriaApp.Components.Inspections._1_AperturaUbicacionFarmacia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._1_AperturaUbicacionFarmacia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.AA:
        case DataModel.Helper.enumAUD_TipoActa.CUA:
            {
                <AuditoriaApp.Components.Inspections._2_AperturaUbicacionAgencia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._2_AperturaUbicacionAgencia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.VF:
            {
                <AuditoriaApp.Components.Inspections._3_RutinaVigilanciaFarmacia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._3_RutinaVigilanciaFarmacia.Index>
                break;
            }
    }
}
