@page "/inspectionsedit/{Id:long}"
@attribute [Authorize]
@layout MainLayout
@implements IDisposable

@if (model != null)
{
    switch (model.Inspeccion.TipoActa){
        case DataModel.Helper.enumAUD_TipoActa.AF:
        case DataModel.Helper.enumAUD_TipoActa.CUF:{
                <AuditoriaApp.Components.Inspections._01_AperturaUbicacionFarmacia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._01_AperturaUbicacionFarmacia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.AA:
        case DataModel.Helper.enumAUD_TipoActa.CUA:
            {
                <AuditoriaApp.Components.Inspections._02_AperturaUbicacionAgencia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._02_AperturaUbicacionAgencia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.VF:
            {
                <AuditoriaApp.Components.Inspections._03_RutinaVigilanciaFarmacia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._03_RutinaVigilanciaFarmacia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.VA:
            {
                <AuditoriaApp.Components.Inspections._04_RutinaVigilanciaAgencia.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._04_RutinaVigilanciaAgencia.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.INV:
            {
                <AuditoriaApp.Components.Inspections._05_Investigaciones.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._05_Investigaciones.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.RR:
            {
                <AuditoriaApp.Components.Inspections._06_RetencionRetiro.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._06_RetencionRetiro.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.COP:
            {
                <AuditoriaApp.Components.Inspections._07_CierreOperaciones.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._07_CierreOperaciones.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.DFP:
            {
                <AuditoriaApp.Components.Inspections._08_DisposicionFinal.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._08_DisposicionFinal.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.AFM:
            {
                <AuditoriaApp.Components.Inspections._09_AperturaFabMedicamento.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._09_AperturaFabMedicamento.Index>
                break;
            }
        case DataModel.Helper.enumAUD_TipoActa.AFC:
            {
                <AuditoriaApp.Components.Inspections._10_AperturaFabCosmetico.Index Id="@Id" BackToMain="BackToMain"></AuditoriaApp.Components.Inspections._10_AperturaFabCosmetico.Index>
                break;
            }
    }
}
