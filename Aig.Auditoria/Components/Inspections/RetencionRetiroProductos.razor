                                            <InputText class="form-control" @bind-Value="Inspeccion.AvisoOperación" />
                                            <ValidationMessage For=@(() => Inspeccion.AvisoOperación) />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <label>@languageContainerService.Keys["OperLicenseNumber"]</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <InputText class="form-control" @bind-Value="Inspeccion.LicenseNumber" />
                                            <ValidationMessage For=@(() => Inspeccion.LicenseNumber) />
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-8">
                                    <label>@languageContainerService.Keys["LegalRepresentation"]</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <InputText class="form-control" @bind-Value="Inspeccion.RepreLegal" />
                                            <ValidationMessage For=@(() => Inspeccion.RepreLegal) />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <label>@languageContainerService.Keys["Identification"]</label>
                                    <div class="form-group">
