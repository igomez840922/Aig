                                                            <SignaturePadFooter>
                                                                <SignaturePadClear>
                                                                    <button type="button" class="btn btn-sm btn-outline-primary waves-effect" @onclick="RemoveSignatureImg10">@languageContainerService.Keys["Clear"]</button>
                                                                </SignaturePadClear>
                                                            </SignaturePadFooter>
                                                        </SignaturePad>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="col-sm-12">
