                                            <InputText class="form-control" @bind-Value="Inspeccion.RepreLegalIdentificacion" />
                                            <ValidationMessage For=@(() => Inspeccion.RepreLegalIdentificacion) />
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-12">
