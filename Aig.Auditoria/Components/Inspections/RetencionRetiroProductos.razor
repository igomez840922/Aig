                                    <div class="form-group">
                                        <div class="form-line">
                                            <InputText class="form-control" @bind-Value="Inspeccion.ParticipantesDNFD" />
                                            <ValidationMessage For=@(() => Inspeccion.ParticipantesDNFD) />
