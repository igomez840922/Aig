                                    }                                    
                                </div>
                            </div>
                            <!--begin::Section-->
                            <div class="kt-section">
                                <div class="table-responsive">
                                    <table class="table rounded table-striped">
                                        <thead class=thead-light>
                                            <tr>
                                                <th>@languageContainerService.Keys["Description"]</th>
                                                <th>@languageContainerService.Keys["File"]</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Inspeccion != null && Inspeccion.LAttachments != null)
                                            {
                                                @foreach (var dt in Inspeccion.LAttachments)
                                                {
                                                    <tr>
                                                        <td scope="row">@dt.Description</td>
                                                        <td>
                                                            <NavLink href="@dt.Url" target="_blank">@languageContainerService.Keys["ViewAttachemnt"]</NavLink>
                                                        </td>
                                                        <td style="text-align:right">
                                                            @if (StatusInspecciones == enum_StatusInspecciones.Pending){
                                                            <button type="button" class="btn btn-danger" @onclick="(e)=>{RemoveAttachment(dt);}">@languageContainerService.Keys["Delete"]</button>

                                                            }
                                                            <!-- /btn-group -->
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
