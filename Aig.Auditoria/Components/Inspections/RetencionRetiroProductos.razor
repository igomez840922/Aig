                        </div>
                        <div class="tab-pane" id="products" role="tabpanel">
                            <h4>@languageContainerService.Keys["Products"]</h4>
                            <div class="row clearfix">
                                <div class="col-9" style="text-align:left;">
                                </div>
                                <div class="col-3" style="text-align:right;">
                                    @if (StatusInspecciones == enum_StatusInspecciones.Pending)
                                    {
                                    <button type="button" @onclick="(e)=>{OpenProduct();}"
                                            class="btn btn-sm btn-outline-primary waves-effect">
                                        @languageContainerService.Keys["Add"]
                                    </button>
                                    }                                    
                                </div>
                            </div>
                            <!--begin::Section-->
                            <div class="kt-section">
                                <div class="table-responsive">
                                    <table class="table rounded table-striped">
                                        <thead class=thead-light>
                                            <tr>
                                                <th>@languageContainerService.Keys["ProductName"]</th>
                                                <th>@languageContainerService.Keys["CommercialPresentation"]</th>
                                                <th>@languageContainerService.Keys["Manufacturer"]</th>
                                                <th>@languageContainerService.Keys["CountryOfManufacture"]</th>
                                                <th>@languageContainerService.Keys["Lot"]</th>
                                                <th>@languageContainerService.Keys["ExpDate"]</th>
                                                <th>@languageContainerService.Keys["AmountWithheld"]</th>
                                                <th>@languageContainerService.Keys["AmountWithdrawal"]</th>
                                                <th>@languageContainerService.Keys["ProductDestiny"]</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Inspeccion != null && Inspeccion.InspRetiroRetencion.LProductos != null)
                                            {
                                                @foreach (var dt in Inspeccion.InspRetiroRetencion.LProductos)
                                                {
                                                    <tr>
                                                        <td scope="row">@dt.Nombre</td>
                                                        <td>@dt.PresentacionComercial</td>
                                                        <td>@dt.Fabricante</td>
                                                        <td>@dt.Pais</td>
                                                        <td>@dt.Lote</td>
                                                        <td>@(dt.FechaExp?.ToString("dd/MM/yyyy") ?? "")</td>
                                                        <td>@dt.CantidadRetenida</td>
                                                        <td>@dt.CantidadRetirada</td>
                                                        <td>@dt.Destino</td>
                                                        <td style="text-align:right">
                                                            @if (StatusInspecciones == enum_StatusInspecciones.Pending){
                                                            <button type="button" class="btn btn-danger" @onclick="(e)=>{RemoveProduct(dt);}">@languageContainerService.Keys["Delete"]</button>

                                                            }
                                                            <!-- /btn-group -->
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane" id="attachments" role="tabpanel">
                            <h4>@languageContainerService.Keys["Attachments"]</h4>
                            <div class="row clearfix">
                                <div class="col-9" style="text-align:left;">
                                </div>
