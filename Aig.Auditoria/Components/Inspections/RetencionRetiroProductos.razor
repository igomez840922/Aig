                                            <ValidationMessage For=@(() => Inspeccion.InspRetiroRetencion.SeccionOficinaRegional) />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <label>@languageContainerService.Keys["NoticeOperation"]</label>
