                                <div class="col-sm-4">
                                    <label>@languageContainerService.Keys["StartDate"]</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <RadzenDatePicker TValue="DateTime" @bind-Value="Inspeccion.FechaInicio" ShowTime="true" HourFormat="12" DateFormat="dd/MM/yyyy h:mm tt" />
                                            <ValidationMessage For=@(() => Inspeccion.FechaInicio) />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <label for="password">@languageContainerService.Keys["Status"]</label>
