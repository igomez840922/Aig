                                                            <td scope="row">@dt.NombreCompleto</td>
                                                            <td>@dt.CedulaIdentificacion</td>
                                                            <td>@dt.RegistroNumero</td>
                                                            <td>@dt.Cargo</td>
                                                            <td style="text-align:right">
