                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="tab" href="#generalInfo" role="tab">@languageContainerService.Keys["GeneralInfo"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#applicantInfo" role="tab">@languageContainerService.Keys["ApplicantInfo"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#regentInfo" role="tab">@languageContainerService.Keys["RegentInfo"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#documentationInfo" role="tab">@languageContainerService.Keys["Personal"]</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#instalaciones" role="tab">@languageContainerService.Keys["Instalaciones"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#controlCalidad" role="tab">@languageContainerService.Keys["Control de Calidad"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#equipos" role="tab">@languageContainerService.Keys["Equipos"]</a>
                        </li>                       
                        
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#conclusionInfo" role="tab">@languageContainerService.Keys["ConclusionInfo"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#inspectorsApprovals" role="tab" @onclick="(e)=>{OnShowSignasure();}">@languageContainerService.Keys["Firmas"]</a>
                        </li>
                    </ul>
                    <div class="tab-content">
