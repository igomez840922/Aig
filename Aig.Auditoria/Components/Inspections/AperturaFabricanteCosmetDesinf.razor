                            </h3>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            <div class="kt-portlet__head-actions">
                                <button type="button" class="btn btn-danger btn-sm btn-bold"
                                @onclick="(e)=>{Cancel();}">
                                    @languageContainerService.Keys["Cancel"]
                                </button>
                            </div>
                        </div>
                    </div>
